<head>

	<script id="slide-template" type="text/x-handlebars-template">
		{% verbatim %}
			{{#each slide}}
				<div class="swiper-slide {{#if hasBigTitle}}title{{/if}}" id="slide{{@index}}">

						{{!--------------------------}}
						{{!-- GESTION DU TEXTE ICI --}}
						{{!--------------------------}}

						{{#if hasEpTitle}}
							<span class="txt-episode">{{hasEpTitle}}</span>
						{{/if}}

						{{#if hasBigTitle}}
							<h1>{{hasBigTitle}}</h1>
						{{/if}}

						{{#if hasBigTitle}}
							  <span class="ca3-scroll-down-link ca3-scroll-down-arrow" data-ca3_iconfont="ETmodules" data-ca3_icon=""></span>
						{{/if}}

						{{#if hasChapterNo}}
							<span class="txt-chapter">{{hasChapterNo}}</span>
						{{/if}}

						{{#if hasChapterTitle}}
							<h2>{{hasChapterTitle}}</h2>
						{{/if}}

						{{#if hasText}}
					    	<div class="text-container">
					    		<span>{{{hasText}}}</span>
					    	</div>
				    	{{/if}}

	  					
						{{!---------------------------------------}}
						{{!-- TEST SI SLIDER PUIS TYPE DE MEDIA --}}
						{{!---------------------------------------}}
						
				    	{{#if isHSlider}}
					    	<div class="swiper-container swipper-nina-horizontal1">
					            <div class="swiper-wrapper">
					              	{{#each hSliderChildren}}
						            <div class="swiper-slide" style="background: url('img/{{this.img}}')">
						            	<div class="text-container">
								    		<span>{{this.text}}</span>
								    	</div>
						            </div>
						            {{/each}}
					            </div>
					            <div class="swiper-pagination"></div>
					        </div>

				        {{else}}

					        <figure {{#if hasGradient}}class="gradient"{{/if}} {{#if hasImgBg}}style="background: url('img/{{hasImgBg}}'); background-position: center center; background-size: cover; background-repeat: no-repeat;{{#if imgContained}}background-size: contain;{{/if}}"{{/if}}>
				    			{{#if isVideo}} 
					    		<video class="video-js vjs-theme-fantasy" muted playsinline {{#if autoplay}}autoplay{{/if}} {{#if loop}}loop{{/if}} preload="metadata" data-setup="{}">
			    					<source src="{{source}}" />
			    					<p class="vjs-no-js">
			      						To view this video please enable JavaScript, and consider upgrading to a web browser that <a href="https://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a>
			      					</p>
			  					</video> 
			  					{{/if}}
			  					{{#if hasQuote}}
		  						<blockquote>{{hasQuote}}<span>{{quoteAuth}}</span></blockquote>
		  						{{/if}}
		  					</figure>

		  					{{#if controls}}
					    		<div class="controls">
					    			<span class="btn btn-sound">SOUN</span>
					    			<span class="btn btn-reset">REST</span>
					    		</div>
					    	{{/if}}

				        {{/if}}
	  					
  						
				    	{{!--------------------}}
						{{!-- OVERLAY OU NON --}}
						{{!--------------------}}

				    	{{#if overlay}}
				    	<div class="overlay"></div>
				    	{{/if}}


				    	{{!---------------------------------}}
						{{!-- SLIDE DE CREDITS EN DUR ICI --}}
						{{!---------------------------------}}

				    	{{#if isCredits}}
				    	<h1> credits</h1>
				    	{{/if}}
			    	
			    </div>
			{{/each}}
		{% endverbatim %}
	</script>

